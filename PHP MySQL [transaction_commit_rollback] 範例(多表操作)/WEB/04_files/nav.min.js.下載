"use strict";$.when($.ready).then(function(){var t=window.location.href.replace(/#.*$/,"").split("/").pop(),e=$('#sidebar a[href *= "'.concat(t,'"]'));e.addClass("current"),$(document).scrollTop(0);try{var a=e.offset().top;a>=.4*$(window).height()&&$("#sidebar").scrollTop(a-.4*$(window).height())}catch(t){}function r(o,i){NProgress.start(),$.get(o,function(t){var e=$(t).filter("title").text(),a=$(t).find("#content").html(),r=$(t).find("#main-nav").html();$("head title").text(e),$("#content").html(a),$("#main-nav").html(r);var n=o.replace(/#.*$/,"").split("/").pop();$("#sidebar a[href]").removeClass("current"),$('#sidebar a[href *= "'.concat(n,'"]')).addClass("current"),$('a[href ^= "http://"]').attr("target","_blank"),$('a[href ^= "https://"]').attr("target","_blank"),$(document).scrollTop(0),NProgress.done(),i&&i()})}$('a[href ^= "http://"]').attr("target","_blank"),$('a[href ^= "https://"]').attr("target","_blank"),$("#sidebar-switch").on("click","button",function(){$("body").toggleClass("sidebar-open"),$("#content-wrap").toggleClass("fixed")}),window.history.replaceState({href:window.location.href},"",window.location.href),$(document).on("click","a",function(t){if($("body").removeClass("sidebar-open"),$("#content-wrap").removeClass("fixed"),!t.metaKey){var e=$(this).attr("href");"#"===e[0]||/^https?:\/\//.test(e)||(t.preventDefault(),r(e,function(){window.history.pushState({href:e},"",e)}))}}),window.onpopstate=function(t){t.state&&r(t.state.href)}});